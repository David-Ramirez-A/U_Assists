<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#E9F2F9" /> <!-- Color de fondo negro -->
        </shape>
    </item>
    <!-- Primera línea vertical -->
    <item android:left="150dp" android:right="150dp" android:gravity="center_vertical">
        <shape android:shape="rectangle">
            <solid android:color="#9CC4E4" /> <!-- Cambia el color de la línea 1 -->
            <size android:width="200dp" android:height="1200dp" /> <!-- Grosor de la línea 1 -->
        </shape>
    </item>
    <!-- Segunda línea vertical -->
    <item android:left="185dp" android:right="185dp" android:gravity="center_vertical">
        <shape android:shape="rectangle">
            <solid android:color="#E9F2F9" /> <!-- Cambia el color de la línea 2 -->
            <size android:width="200dp" android:height="1200dp" /> <!-- Grosor de la línea 2 -->
        </shape>
    </item>
</layer-list>
